{
    "name": "pedrotroller/php-cs-custom-fixer",
    "description": "PHP-CS-FIXER : my custom fixers",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "friendsofphp/php-cs-fixer": "~1.13",
        "symfony/yaml": "~2.8|~3.0"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "phpspec/phpspec": "~3.0"
    },
    "autoload": {
        "psr-0": { "PedroTroller\\CS": "src/" }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev"
        }
    }
}
