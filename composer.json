{
    "name": "pedrotroller/php-cs-custom-fixer",
    "description": "PHP-CS-FIXER : my custom fixers",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "friendsofphp/php-cs-fixer": "~2.0"
    },
    "require-dev": {
        "mustache/mustache": "^2.12"
    },
    "autoload": {
        "psr-4": { "PedroTroller\\CS\\": "src/PedroTroller/CS/" }
    },
    "autoload-dev": {
        "psr-4": { 
            "tests\\": "tests/" 
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7.x-dev"
        }
    },
    "scripts": {
        "tests": [
            "tests\\Runner::run"
        ]
    }
}
